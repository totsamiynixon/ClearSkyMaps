
@{
    ViewBag.Title = "Readings";
}
<div id="app">
    <div class="side-bar-holder">
        @await Html.PartialAsync("_Partials/Sidebar")
    </div>
    <div class="inner-content">
        <div class="map-holder">
            <div id="map"></div>
        </div>
    </div>
    @await Html.PartialAsync("_Partials/DetailsModal")
    @await Html.PartialAsync("_Partials/ContextMenu")
</div>
@section Scripts {
    <script src="~/lib/Chart.bundle.min.js"></script>
    <script src="~/lib/moment.js"></script>
    <script src="~/lib/vue.js"></script>
    <!--Ссылка на библиотеку SignalR -->
    <script src="~/lib/signalr.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAfj-ARjZc7VEGb0_grdk5VFu5wXphQyjo&libraries=geometry&callback"></script>
    <script src="~/lib/richmarker.js"></script>
    <script src="~/lib/front/vue-filters.js"></script>
    <script src="~/lib/front/readings.js"></script>
}