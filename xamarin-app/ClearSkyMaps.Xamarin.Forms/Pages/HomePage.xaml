<?xml version="1.0" encoding="utf-8" ?>
<base:HomePageXaml
             xmlns:base="clr-namespace:ClearSkyMaps.Xamarin.Forms.Pages"
             xmlns:map="clr-namespace:ClearSkyMaps.Xamarin.Forms.Controls.CustomMap"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ClearSkyMaps.Xamarin.Forms.Pages.HomePage">
    <base:HomePageXaml.ToolbarItems>
        <ToolbarItem Order="Primary" Text="Details" Clicked="ToolbarItem_Clicked"></ToolbarItem>
    </base:HomePageXaml.ToolbarItems>
    <base:HomePageXaml.Content>
        <map:CustomMap x:Name="Map" Circles="{Binding Circles, Mode=TwoWay}"></map:CustomMap>
    </base:HomePageXaml.Content>
</base:HomePageXaml>